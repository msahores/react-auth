{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "react/jsx-filename-extension": [0, { "extensions": [".js", ".jsx", ".tsx"] }],
      "no-use-before-define": 0,
      "import/no-unresolved": 0,
      "import/extensions": 0,
      "comma-dangle": 1,
      "arrow-parens": 0
    }
}